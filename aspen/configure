rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
      fi
      CXXFLAGS=$old_cxxflags
   done
   if test -z "$OPENMP"; then
      { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: attempts to enable OpenMP failed; disabling" >&5
$as_echo "$as_me: WARNING: attempts to enable OpenMP failed; disabling" >&2;};
      OPENMP="no";
   fi
elif test "$OPENMP" != "no"; then
   old_cxxflags=$CXXFLAGS
   # Again, add -Werror to make check successful (see above).
   CXXFLAGS="$CXXFLAGS -Werror $OPENMP"
   { $as_echo "$as_me:${as_lineno-$LINENO}: checking if OpenMP support is enabled by $OPENMP" >&5
$as_echo_n "checking if OpenMP support is enabled by $OPENMP... " >&6; }
   cat confdefs.h - <<_ACEOF >conftest.$ac_ext
/* end confdefs.h.  */
#include <omp.h>
int
main ()
{
int *x=new int[1024], *y=new int[1024];
                #pragma omp parallel for
                for (int i=0; i<1024; ++i) {x[i]=y[i]*2;}

  ;
  return 0;
}
_ACEOF
if ac_fn_cxx_try_link "$LINENO"; then :
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: yes" >&5
$as_echo "yes" >&6; }
else
  { $as_echo "$as_me:${as_lineno-$LINENO}: result: no" >&5
$as_echo "no" >&6; };OPENMP="no";{ $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: given flags did not enable OpenMP; disabling" >&5
$as_echo "$as_me: WARNING: given flags did not enable OpenMP; disabling" >&2;}
fi
rm -f core conftest.err conftest.$ac_objext \
    conftest$ac_exeext conftest.$ac_ext
   CXXFLAGS=$old_cxxflags
fi

# We're going to use CXXFLAGS for the OpenMP flags
