{ $as_echo "$as_me:${as_lineno-$LINENO}: checking for MPI" >&5
$as_echo_n "checking for MPI... " >&6; }
{ $as_echo "$as_me:${as_lineno-$LINENO}: result: $MPI" >&5
$as_echo "$MPI" >&6; }

if test "$detect_mpi_flags" = "yes"; then
   # extract MPI compile info from compiler
   if test "$MPI" != "no"; then
      # option 1: -compile-info
      MPISHOW_C=`$MPI -compile-info 2>&5`
      # option 2: -show
      if test "$?" -ne "0"; then
         MPISHOW_C=`$MPI -show 2>&5`
      fi
      if test "$?" -ne "0"; then
         MPISHOW_C=`$MPI --cray-print-opts=cflags 2>&5`
         if test "$?" -ne "0"; then
            MPI="no"
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not extract compile flags from MPI compiler; disabling MPI" >&5
$as_echo "$as_me: WARNING: could not extract compile flags from MPI compiler; disabling MPI" >&2;}
         fi
      fi

   fi

   # extract MPI link info from compiler
   if test "$MPI" != "no"; then
      # option 1: -link-info
      MPISHOW_L=`$MPI -link-info 2>&5`
      # option 2: -show
      if test "$?" -ne "0"; then
         MPISHOW_L=`$MPI -show 2>&5`
      fi
      if test "$?" -ne "0"; then
         MPISHOW_L=`$MPI --cray-print-opts=libs 2>&5`
         if test "$?" -ne "0"; then
            MPI="no"
            { $as_echo "$as_me:${as_lineno-$LINENO}: WARNING: could not extract link flags from MPI compiler; disabling MPI" >&5
$as_echo "$as_me: WARNING: could not extract link flags from MPI compiler; disabling MPI" >&2;}
         fi
      fi
   fi

   # turn raw MPI info into actual flags
   if test "$MPI" != "no"; then
      MPI_CPPFLAGS=`echo $MPISHOW_C | perl -e '$_=<>;print join(" ",grep(/^-I/,split(/\s+/)));'`
      #MPI_LDFLAGS=`echo $MPISHOW_L | perl -e '$_=<>;print join(" ",grep(!/^-I|^-l|^\w/,split(/\s+/)));'`
      MPI_LDFLAGS=`echo $MPISHOW_L | perl -e '$_=<>;print join(" ",grep(/^-L/,split(/\s+/)));'`
      MPI_LIBS=`echo $MPISHOW_L | perl -e '$_=<>;print join(" ",grep(/^-l/,split(/\s+/)));'`
   fi
fi

# now actually try MPI...
if test "$MPI" != "no"; then
   old_cppflags=$CPPFLAGS
   old_ldflags=$LDFLAGS
   old_libs=$LIBS
   CPPFLAGS="$CPPFLAGS $MPI_CPPFLAGS";
   LDFLAGS="$LDFLAGS $MPI_LDFLAGS";
   LIBS="$LIBS $MPI_LIBS"

   { $as_echo "$as_me:${as_lineno-$LINENO}: checking if MPI works" >&5
$as_echo_n "checking if MPI works... " >&6; }
   cat confdefs.h - <<_ACEOF >conftest.$ac_ext
