model spmul{

param n = 1

// Map Data to Machine Components with resource load/store
data {

     map colind to NVM
     map values to NVM
     map x      to HPM0
     map rowptr to NVM
     map y      to NVM
}

// Map Kernel Execution to Machine Components with resource flops
kernel{
     map spmul to Core_S
}

}

